{
  "name": "@daniildev/trading-vue3-js",
  "version": "1.0.4",
  "type": "module",
  "description": "Customizable charting lib for traders. Vue 3 + Vite. Based on https://github.com/C451/trading-vue-js by C451.",
  "main": "./dist/trading-vue.umd.js",
  "module": "./dist/trading-vue.es.js",
  "types": "./dist/trading-vue.d.ts",
  "exports": {
    ".": {
      "types": "./dist/trading-vue.d.ts",
      "import": "./dist/trading-vue.es.js",
      "require": "./dist/trading-vue.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "test": "vite serve test --mode development",
    "auto-test": "ava",
    "eslint": "./node_modules/.bin/eslint --ext .js,.vue src",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "author": "SaViGnAnO",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE.md",
    "MIGRATION_COMPLETE.md"
  ],
  "keywords": [
    "trading",
    "vue",
    "vue3",
    "charts",
    "crypto",
    "visualization",
    "reactive",
    "algo-trading",
    "finviz",
    "trading-systems",
    "vite",
    "candlestick",
    "technical-analysis"
  ],
  "homepage": "https://github.com/SaViGnAnO/trading-vue3-js#readme",
  "repository": "github:SaViGnAnO/trading-vue3-js",
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.25.0",
    "@babel/plugin-transform-parameters": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/register": "^7.25.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "ava": "^5.3.1",
    "eslint": "^9.8.0",
    "eslint-config-eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.27.0",
    "vite": "^5.0.0",
    "vue-codemirror": "^4.0.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "arrayslicer": "^1.2.3",
    "hammerjs": "^2.0.8",
    "hamsterjs": "^1.1.3",
    "lz-string": "^1.4.4",
    "moment": "^2.30.1",
    "vue": "^3.4.38"
  }
}
